	option config_reload '1'
	option operation_mode 'redir-host'

config dns_servers
	option enabled '1'
	option group 'nameserver'
	option type 'udp'
	option ip '127.0.0.1'
	option port '10053'

config config_subscribe
	option enabled '1'
	option name 'github_share'
	option address 'https://raw.githubusercontent.com/ssrsub/ssr/master/Clash.yml'
	option sub_convert '1'
	option template 'Default'
	option emoji 'false'
	option udp 'true'
	option skip_cert_verify 'true'
	option sort 'true'
	option node_type 'false'

