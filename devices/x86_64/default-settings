
	uci del turboacc.config.sw_flow
	uci commit turboacc
	uci set system.@system[0].zram_size_mb='100'
	uci set system.@system[0].zram_comp_algo='lz4'
	uci commit system

touch /etc/config/placeholder

sed -i '$a src/gz openwrt_packages2 https://op.supes.top/19.07.4/packages/x86_64/packages' /etc/opkg/distfeeds.conf
sed -i '$a src/gz openwrt_base2 https://op.supes.top/19.07.4/packages/x86_64/base' /etc/opkg/distfeeds.conf
sed -i '$a src/gz openwrt_custom https://op.supes.top/packages/x64' /etc/opkg/distfeeds.conf

exit 0
