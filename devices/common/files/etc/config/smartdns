
config smartdns
	option seconddns_tcp_server '1'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_cache '0'
	option rr_ttl_max '86400'
	option tcp_server '1'
	option coredump '1'
	option cache_size '50480'
	option server_name 'smartdns'
	option seconddns_no_speed_check '1'
	option ipv6_server '0'
	option prefetch_domain '1'
	option enabled '1'
	option serve_expired '1'
	option dualstack_ip_selection '0'
	option redirect 'dnsmasq-upstream'
	option rr_ttl_min '1800'
	option anti_ad '0'
	option seconddns_enabled '0'
	option seconddns_port '5335'
	option seconddns_server_group 'oversea'
	option seconddns_no_rule_soa '1'
	option seconddns_no_dualstack_selection '1'
	option force_aaaa_soa '1'
	option port '5555'
	list old_redirect 'dnsmasq-upstream'
	list old_port '5555'
	list old_enabled '1'

config server
	option name 'DNSPod'
	option type 'https'
	option ip 'https://doh.pub/dns-query'
	option addition_arg ' -check-edns'
	option enabled '1'
	option blacklist_ip '0'
	option no_check_certificate '0'

config server
	option name 'AliDNS'
	option type 'https'
	option ip 'https://dns.alidns.com/dns-query'
	option addition_arg ' -check-edns'
	option enabled '1'
	option blacklist_ip '0'
	option no_check_certificate '0'

config server
	option name '114'
	option type 'tcp'
	option ip '114.114.114.114'
	option blacklist_ip '1'
	option addition_arg ' -check-edns'
	option enabled '1'

config server
	option name 'Cloudflare'
	option type 'https'
	option server_group 'oversea'
	option blacklist_ip '0'
	option no_check_certificate '0'
	option enabled '0'
	option ip 'https://1.1.1.1/dns-query'
	option addition_arg '-host-name cloudflare-dns.com -check-edns'

config server
	option name 'Google'
	option type 'https'
	option server_group 'oversea'
	option blacklist_ip '0'
	option no_check_certificate '0'
	option ip 'https://8.8.8.8/dns-query'
	option addition_arg '-host-name dns.google -check-edns'
	option enabled '0'
