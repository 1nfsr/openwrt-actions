config AdGuardHome 'AdGuardHome'
	option configpath '/etc/AdGuardHome.yaml'
	option workdir '/usr/bin/AdGuardHome'
	option logfile '/tmp/AdGuardHome.log'
	option binpath '/usr/bin/AdGuardHome/AdGuardHome'
	option keepdb '1'
	option httpport '100'
	option autoupdate '1'
	option backupwd '0'
	option cutquerylog '1'
	option lastlogfile '/tmp/AdGuardHome.log'
	option crontab 'autoupdate cutquerylog cutruntimelog'
	option waitonboot '0'
	option upprotect '$configpath $workdir/data/sessions.db $workdir/data/stats.db'
	option enabled '1'
	option ucitracktest '1'
